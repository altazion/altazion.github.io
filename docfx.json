{
  "metadata": [],
  "build": {
    "content": [
      {
        "files": [
          "**/toc.yml",
          "**.md",
          "ecommerce/controls/**.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "cnam*.*"
          ,"CNAM*.*"
          ,"CNAME"
          ,"index.html"
          ,"**/images/**"
          ,"**.jpg"
          ,"**.jpeg"
          ,"**.png"
          ,"**.bmp"
          ,"**.ico"
          ,"**.gif"
          ,"**.mov"
          ,"**.mkv"
          ,"**.mp4"
          ,"**.pptx"
          ,"**.css"
          ,"**.js"
          ,"**.ttf"
          ,"**.woff"
          ,"**.svg"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",  "templates/altazion"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "sitemap":{
      "baseUrl": "https://www.altazion.dev/",
      "priority": 0.3,
      "changefreq": "monthly",
      "fileOptions":{
          "**.yml": {
              "priority": 0.5
          }
      }
    }
  }
}
